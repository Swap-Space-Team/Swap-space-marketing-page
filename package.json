{
  "name": "swapspace-marketing-site",
  "version": "1.0.0",
  "description": "Swap Space Marketing Website",
  "main": "Index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=/Index.html --watch=.",
    "start": "live-server --port=3000 --watch=.",
